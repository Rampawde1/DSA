class Solution
{
    //Function to return the total number of possible unique BST.
    static int numTrees(int N)
    {
        // Your code goes here
        long MOD = 1000000007;
        long[] dp = new long[N + 1];
        
        dp[0] = 1;//by default 1
        dp[1] = 1;
        
        for(int i = 2; i <= N; i++){
            for(int left = 0; left <= i - 1; left++){
                int right = i - left - 1;
                
                //dp[i] += dp[left] * dp[right];
                
                long val = (dp[left] * dp[right]) % MOD;
                
                dp[i] = (dp[i] + val) % MOD;
            }
        }
        return (int)dp[N];
    }
}
